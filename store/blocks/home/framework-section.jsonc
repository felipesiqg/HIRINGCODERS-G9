  "rich-text#deskframetexto1": {
    "props": {
      "text": "A Corebiz atua em toda jornada digital do usuário. Desde a \n configuração de plataformas, implementações e integrações, até a \n entrega de uma excelente experiência de consumo.",
      "font": "t-heading-7",
      "textPosition": "CENTER",
      "textAlignment": "LEFT",
      "blockClass": "deskframetexto1",
      "textColor": "c-on-emphasis"
    }
  },
  "rich-text#deskframetexto2": {
    "props": {
      "text": "Juntamente com toda a equipe de marketing trabalhando para uma \n melhor performance digital, o que qualifica ainda mais a entrega da \n jornada do usuário.",
      "font": "t-heading-7",
      "textPosition": "CENTER",
      "textAlignment": "LEFT",
      "blockClass": "deskframetexto2",
      "textColor": "c-on-emphasis"
