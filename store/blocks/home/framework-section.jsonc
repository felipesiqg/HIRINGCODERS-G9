  // DESKTOP

  "stack-layout#deskframework": {
    "children": [
      "image#deskframefundo",
      "image#deskframe",
      "rich-text#deskframetitulo",
      "rich-text#deskframetexto1",
      "rich-text#deskframetexto2",
      "image#deskframemouse",
