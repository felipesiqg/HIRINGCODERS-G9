      "textColor": "c-on-second-emphasis"
    }
  },
  "rich-text#deskframetexto5": {
    "props": {
      "text": "experience \n technology \n marketing",
      "font": "t-heading-6",
      "textPosition": "CENTER",
      "textAlignment": "LEFT",
